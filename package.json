{
  "name": "laptop-friendly",
  "version": "1.0.0",
  "description": "a directory for laptop friendly places",
  "main": "index.html",
  "scripts": {
    "build": "webpack",
    "watch": "concurrently --kill-others \"webpack --watch\" \"npm run serve\"",
    "serve": "node server.js",
    "start": "npm run build && npm run serve",
    "test": "NODE_ENV=test karma start --log-level debug",
    "test:watch": "NODE_ENV=test karma start --auto-watch --no-single-run"
  },
  "keywords": [
    "angularjs",
    "html",
    "es6"
  ],
  "author": "Ren√© Olivo",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "concurrently": "^3.1.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "live-server": "^1.1.0",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.3.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.2"
  }
